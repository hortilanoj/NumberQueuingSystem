<Window x:Class="NumberQueuingSystem.FRONTDESK.Views.vPromptTicket"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="" Height="384.091" Width="833.459" WindowStyle="None" Background="White" WindowStartupLocation="CenterScreen" ResizeMode="NoResize">
    <Grid>
        <Grid.Background>
            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                <GradientStop Color="#FF010781" Offset="0.109"/>
                <GradientStop Color="White" Offset="0.492"/>
                <GradientStop Color="#FF010781" Offset="0.819"/>
            </LinearGradientBrush>
        </Grid.Background>
        <Grid Margin="5" Background="White">
            <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
                <TextBlock HorizontalAlignment="Center" Margin="5" TextWrapping="Wrap" Text="YOUR NUMBER IS" VerticalAlignment="Center" FontWeight="Bold" FontSize="80"/>
                <TextBlock HorizontalAlignment="Center" Margin="5" TextWrapping="Wrap" Text="{Binding TransactionQueue}" VerticalAlignment="Center" FontWeight="Bold" FontSize="100" Foreground="#FF010781"/>
            </StackPanel>
        </Grid>
    </Grid>
</Window>
